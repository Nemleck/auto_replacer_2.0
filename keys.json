{
    "global_params": {
        "max_keys": 50,
        "default_config_params": {
            "keep_text": false,
            "case_sensitive": true,
            "separator": null,
            "operation": null
        }
    },
    
    "special_keys": {
        "end": "²end",
        "reload": "²r",
        "on": "²on",
        "off": "²off",
        "hey": "²hey",
        "clear": "²clear"
    },

    "external_keys": [
        "texts.json"
    ],

    "keys": [
        {
            "input": "²sent1",
            "output": "You can also make pre-written sentences !",
            "params": {
                "case_sensitive": false
            }
        },
        {
            "input": "print(arg)",
            "output": "I print : '(msg)'",
            "params": {
                "separator": "()",
                "operation": {
                    "msg": "'Coucou' + '(arg)'"
                }
            }
        },
        {
            "input": "²search{theme}",
            "output": "Voici ce que j'ai trouvé sur le wiki :\n{part_1}\n{part_2}",
            "params": {
                "case_sensitive": false,
                "separator": "{}",
                "operation": {
                    "res": "get_wiki_info('{theme}')",
                    "part_1": "{res}[0]",
                    "part_2": "{res}[1]"
                }
            }
        },
        {
            "input": "²calc{calcul}",
            "output": "{calcul} = {result}",
            "params": {
                "case_sensitive": false,
                "separator": "{}",
                "operation": {
                    "result": "{calcul}"
                }
            }
        },
        {
            "input": "²configs",
            "output": "{result}",
            "params": {
                "separator": "{}",
                "operation": {
                    "result": "self.get_stringed_configs()"
                }
            }
        }
    ]
}